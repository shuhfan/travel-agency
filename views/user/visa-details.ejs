            <div class="row g-lg-4 gy-5">
                <div class="col-lg-8">
                    <div class="visa-thumb">
                        <img src="<%= visa.images[0] %>" alt="<%= visa.title %>">
                    </div>
                    <div class="visa-title">
                        <h3><%= visa.title %></h3>
                    </div>
                    <ul class="visa-meta">
                        <li><span>Country :</span> <%= visa.location %></li>
                        <li><span>Visa Type :</span> <%= visa.visaType %></li>
                        <li><span>Maximum Stays :</span> 30 Days</li> <!-- Assuming this is a static value -->
                        <li><span>Processing Time :</span> <%= visa.processingTime %></li>
                        <li><span>Validity :</span> <%= visa.validity %></li>
                        <li><span>Visa Mode :</span> <%= visa.visaMode %></li>
                    </ul>
                    
