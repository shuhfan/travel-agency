<%-include('../partials/adminHeader')%>
        <div class="main-content">
            <div class="row">
                <div class="col-xl-12">
                    <div class="main-content-title-profile mb-50">
                        <div class="main-content-title">
                            <h3>Add New Visa</h3>
                        </div>
                    </div>
                    <div class="dashboard-profile-wrapper two">
                        <div class="dashboard-profile-tab-content">
                            <form action="/admin/visa-upload" method="POST" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label for="title">Add Title</label>
                                            <input type="text" id="title" name="title" placeholder="Title here..." required>
                                        </div>
                                    </div>
                                    <div class="col-md-6 mb-30">
                                        <div class="form-inner">
                                            <label for="visaType">Visa Type</label>
                                            <input type="text" id="visaType" name="visaType" placeholder="Visa type..." required>
                                        </div>
                                    </div>
                                    <div class="col-md-6 mb-30">
                                        <div class="form-inner">
                                            <label for="visaMode">Visa Mode</label>
                                            <select id="visaMode" name="visaMode" required>
                                                <option value="Electronic Visa (e-Visa)">Electronic Visa (e-Visa)</option>
                                                <option value="Visa on Arrival (VOA)">Visa on Arrival (VOA)</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-6 mb-30">
                                        <div class="form-inner">
                                            <label for="validity">Validity</label>
                                            <input type="" id="validity" name="validity" placeholder="Validity here..." required>
                                        </div>
                                    </div>
                                    <div class="col-md-6 mb-30">
                                        <div class="form-inner">
                                            <label for="processingTime">Processing Time</label>
                                            <input type="text" id="processingTime" name="processingTime" placeholder="Processing time here..." required>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label for="price">Price</label>
                                            <input type="number" id="price" name="price" placeholder="Price here..." required>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-inner mb-30">
                                            <label for="location">Location</label>
                                            <input type="text" id="location" name="location" placeholder="Location here..." required>
                                        </div>
                                    </div>
                                    <div class="col-md-12 mb-30">
                                        <div class="form-inner">
                                            <label for="description">Description</label>
                                            <textarea id="description" name="description" placeholder="Description here" required></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="upload-img-area">
                                    <div class="upload-img-wrapper">
                                        <input type="file" multiple id="image" name="images" >
                                        
                                    </div>
                                    <div class="upload-img-area-content">
                                        <h6>Upload Your Image</h6>
                                        <p>Image required size 300*300, JPEG or PNG format.</p>
                                    </div>
                                </div>
                                
                                <div class="form-inner">
                                    <button type="submit" class="primary-btn3">Publish Now</button>
                                </div>
                            </form>                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%-include('../partials/adminFooter')%>