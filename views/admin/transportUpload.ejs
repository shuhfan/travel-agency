<%-include('../partials/adminHeader')%>
        <div class="main-content">
            <div class="row">
                <div class="col-xl-12">
                    <div class="main-content-title-profile mb-50">
                        <div class="main-content-title">
                            <h3>Add New Transports</h3>
                        </div>
                    </div>
                    <div class="dashboard-profile-wrapper two">
                        <div class="dashboard-profile-tab-content">
                            <form action="/submit-transport" method="POST" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label>Transport Name</label>
                                            <input type="text" name="transportName" placeholder="Transport name here...">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label>Type</label>
                                            <input type="text" name="type" placeholder="Type here...">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label>Price</label>
                                            <input type="number" name="price" placeholder="Price here...">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label>Discount Price</label>
                                            <input type="number" name="discountPrice" placeholder="Discount price here...">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label>Duration</label>
                                            <input type="text" name="duration" placeholder="Duration here...">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-inner mb-30">
                                            <label>Number Of People (max)</label>
                                            <input type="number" name="numberOfPeople" placeholder="Number of people here...">
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-inner mb-30">
                                            <label>Location</label>
                                            <input type="text" name="location" placeholder="Location here...">
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-12 mb-30">
                                        <div class="form-inner">
                                            <label>Description</label>
                                            <textarea name="description" placeholder="Description here..."></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="upload-img-area">
                                    <div class="upload-img-wrapper">
                                        <div > 
                                            <input type="file" name="images" multiple>
                                        </div>
                                    </div>
                                    <div class="upload-img-area-content">
                                        <h6>Upload Your Image</h6>
                                        <p>Image required size 300*300, JPGE or PNG format.</p>
                                    </div>
                                </div>
                                
                                <div class="form-inner">
                                    <button type="submit" class="primary-btn3">Publish Now</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%-include('../partials/adminFooter')%>